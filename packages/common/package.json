    },
    {
      "name": "John Hauck",
      "email": "jhauck@esri.com"
    },
    {
      "name": "Dave Bouwman",
      "email": "dbouwman@esri.com"
    },
    {
      "name": "John Gravois"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/solution.js/issues"
  },
  "homepage": "https://github.com/Esri/solution.js#readme",
  "keywords": [
    "typescript",
