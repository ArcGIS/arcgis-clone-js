{
  "name": "@esri/solution-creator",
  "version": "0.0.1",
  "description": "to do",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/common.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@esri/arcgis-rest-auth": "^1.17.1",
    "@esri/arcgis-rest-common-types": "^1.17.1",
    "@esri/arcgis-rest-feature-service-admin": "^1.17.1",
    "@esri/arcgis-rest-groups": "^1.17.1",
    "@esri/arcgis-rest-items": "^1.17.1",
    "@esri/arcgis-rest-request": "^1.17.1",
    "@esri/arcgis-rest-sharing": "^1.17.1",
    "adlib": "^3.0.0",
    "tslib": "^1.9.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.json --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc -p ./tsconfig.json --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/solution.js.git"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/Esri/solution.js/issues"
  },
  "homepage": "https://github.com/Esri/solution.js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ],
  "devDependencies": {
    "rollup": "^1.6.0"
  }
}
