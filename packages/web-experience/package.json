{
  "name": "@esri/solution-web-experience",
  "version": "0.21.5",
  "description": "Manages the creation and deployment of Web Experience item types for @esri/solution.js.",
  "main": "dist/node/index.js",
  "unpkg": "dist/umd/web-experience.umd.min.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "author": "Esri",
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^2.24.0",
    "@esri/arcgis-rest-feature-layer": "^2.24.0",
    "@esri/arcgis-rest-portal": "^2.24.0",
    "@esri/arcgis-rest-request": "^2.24.0",
    "@esri/arcgis-rest-service-admin": "^2.24.0",
    "@esri/hub-common": "^6.20.1",
    "@esri/hub-initiatives": "^6.20.1",
    "rollup": "^1.22.0",
    "typescript": "4.0.5"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-auth": "^2.24.0",
    "@esri/arcgis-rest-feature-layer": "^2.24.0",
    "@esri/arcgis-rest-portal": "^2.24.0",
    "@esri/arcgis-rest-request": "^2.24.0",
    "@esri/arcgis-rest-service-admin": "^2.24.0",
    "@esri/hub-common": "^6.20.1",
    "@esri/hub-initiatives": "^6.20.1"
  },
  "dependencies": {
    "@esri/solution-common": "^0.21.5",
    "@esri/solution-simple-types": "^0.21.5",
    "tslib": "^2.0.3"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc -p ./tsconfig.json --module esnext --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ../../umd-base-profile.js && rollup -c ../../umd-production-profile.js",
    "build:node": "tsc -p ./tsconfig.json --module commonjs --outDir ./dist/node",
    "dev:esm": "tsc -w --module esnext --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ../../umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/solution.js.git"
  },
  "contributors": [
    {
      "name": "Dave Bouwman",
      "email": "dbouwman@esri.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/Esri/solution.js/issues"
  },
  "homepage": "https://github.com/Esri/solution.js#readme",
  "keywords": [
    "typescript",
    "promise",
    "fetch",
    "arcgis",
    "esri",
    "ES6"
  ],
